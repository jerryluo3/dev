.footer-container{
    width:100%;
    position:fixed;
    bottom:0;
    height:100rpx;
    background-color:#eee;
    z-index:99;
}
    .footer-container .icon-container{text-align:center;display:flex;}
        .icon-container navigator{width:100%;height:100rpx;box-sizing:border-box;padding:10rpx 0;display:flex;flex-direction:column;justify-content:center;}
        .icon-container image.icon{width:40rpx;height:40rpx;margin:0 auto;}
        .icon-container .text{font-size:24rpx;}
        .icon-container.active{color:black}