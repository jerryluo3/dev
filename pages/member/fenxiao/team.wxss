.team_bar{background:#fff;border-bottom:1rpx solid #ccc;height:100rpx;line-height:100rpx;text-align:center;margin-bottom:20rpx;}
.selected{color:#f00;}

.team_list{background:#fff;}
.teamer{width:24.5%;display:inline-block;text-align:center; vertical-align: top;border-right:1rpx solid #eee;border-bottom:1rpx solid #eee;padding:10rpx 0;}
