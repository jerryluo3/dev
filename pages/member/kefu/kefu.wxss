/* pages/member/kefu/kefu.wxss */

.kefu_block{background:#fff;padding:30rpx;display:block;}
.kefu_btn{position:relative;width:100%;height:100rpx;color:#6d6d6d;}
.member_kefu{position:absolute;top:0;right:0;left:0;bottom:0;}
.kefu_btn_text{width:100%;background:#d3bea5;color:#fff;height:100rpx;line-height:100rpx;font-weight:36rpx;text-align:center;border-radius:10rpx;}
.kefu_time{width:100%;padding:40rpx 0 0;text-align:center;display:inline-block;vertical-align:top;font-size:32rpx;color:#6d6d6d}
.kefu_ico{background:#d3bea5;padding:10rpx;border-radius:50%;width:40rpx;height:40rpx;display:inline-block;vertical-align:middle;}
.kefu_ico image{width:40rpx;height:40rpx;}
.kefu_tips{text-align:center;width:100%;font-size:32rpx;color:#6d6d6d;padding:10rpx 0 40rpx;}

.kefu_content{background:#fff;padding:30rpx;}
.kefu_content_title{height:40rpx;line-height:40rpx;font-size:32rpx;margin-bottom:30rpx;}
.kefu_content_ico{width:36rpx;height:36rpx;display:inline-block; vertical-align: middle;background:#d3bea5;padding:10rpx;border-radius:50%;}
.kefu_content_ico image{width:36rpx;height:36rpx;display:block;}
.kefu_content_c{font-size:28rpx;}

.wxParse-img{width:100% !important;}