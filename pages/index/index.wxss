/**index.wxss**/

.topblank{width: 100%;}

.tab-h{height: 80rpx;width: 100%; box-sizing: border-box;overflow: hidden;line-height: 80rpx;background: #fff; font-size: 16px; white-space: nowrap;}
.tab-item{color:#595757;}
.tab-item.active{color: #e40064;position: relative;}
/* .tab-item.active:after{ content: "";display: block;height: 4rpx;width: 100%;background: #e40064;position: absolute; bottom: 0;left: 0rpx;} */
.weui-grids{background-color:white;}
.weui-grid{padding:10rpx 0;border:none;width:25%;}
.weui-grid__icon{width:100rpx;height:100rpx;}
.item-ans{ width: 100%;display: flex; flex-grow: row no-wrap;justify-content: space-between; padding: 30rpx;box-sizing: border-box; height: 180rpx;align-items: center;border-bottom: 4rpx solid #e40064;}
.fixed{position:fixed;top:0;z-index:1000;}


.swiper {width: 100%;height: 350rpx;}
.swiper swiper-item{width: 100%;height:350rpx;}

.sanbao{padding:10rpx;border-bottom:1rpx solid #eee;font-size:26rpx;height:48rpx;line-height:48rpx;background:#fff;color:#595757;margin-top:10rpx;
}
.sanbao .sanbao_ico{width:32rpx;height:34rpx;display:inline-block; vertical-align:middle;margin:0 6rpx;}
.sanbao_detail{background:#d8edf1;border:1rpx solid #5cc1cf;width:80rpx;border-radius:30rpx;float:right;line-height:40rpx;text-align:center;font-size:24rpx;}

.vip_ads{width:100%;}
.vip_ads image{width:100%;height:200rpx;}

.tggp{width:100%;background:#fff;}
.tggp_title{width:100%;text-align:center;margin:10rpx 0;padding:30rpx 0;font-weight:bold;border-bottom:1rpx solid #eee;position:relative;}
.tggp_title image{width:60rpx;height:60rpx;display:inline-block; vertical-align:middle;}
.tggp_title .tggp_more{right:10rpx;padding:15rpx 0;font-size:24rpx;font-weight:normal;position:absolute;top:26rpx;}
.tggp_title .tggp_more image{width:30rpx;height:30rpx;display:inline-block; vertical-align: middle;}

.tggp_list{width:100%;box-sizing: border-box;overflow: hidden;line-height: 80rpx;background: #fff; white-space: nowrap;height:350rpx;}
.tggp_block{position:relative;margin:10rpx;border-radius:6rpx;display:inline-block;height:440rpx;}
.tggp_block,.tggp_block image{width:232rpx;height:232rpx;}
.tggp_block_ico{position:absolute;top:0;left:0;}
.tggp_block_ico image{width:120rpx;height:107rpx;}
.tggp_block image{border-radius:6rpx;}
.tggp_block .tggp_block_footer{position:absolute;left:0;bottom:-80rpx;height:80rpx;line-height:80rpx;text-align:center;background:#a5dee5;color:#000;width:100%;font-size:24rpx; border-bottom-left-radius:6rpx;border-bottom-right-radius:6rpx;}
.tggp_block .tggp_block_footer text{margin:0 5rpx;padding:10rpx;font-size:28rpx;font-weight:bold;}
.tggp_block .tggp_block_footer .tggp_tname{padding-left:20rpx;text-align:left;font-weight:bold;}
.tggp_block .tggp_block_footer .sy{padding-right:20rpx;font-size:20rpx;}

.tggp_block .dtime{text-align:center;position:absolute;width:100%;bottom:0;}
.tggp_block .dtime{background-color:rgba(0, 0, 0, 0.6);color:#fff;display:inline-block; box-shadow:none;padding:0 10rpx;font-size:24rpx;}
.tggp_block .timeout{position:relative;z-index:10;width:100%;text-align:center;margin:-40rpx auto 0;background-color:rgba(0, 0, 0, 0.7);color:#fff;height:40px;line-height:40px;}
.tggp_block .ltime{width:100%;text-align:center;position:absolute;bottom:0rpx;}
.tggp_block .ltime text{background-color:rgba(0, 0, 0, 0.5);color:#fff;display:inline-block; box-shadow:none;padding:0 6px;height:44rpx;line-height:44rpx;margin:0 3rpx;font-size:20rpx;}

.main_block{width:100%;background:#fff;margin-top:20rpx;}

.main_block_title{text-align:center;width:100%;border-bottom:1rpx solid #ccc;height:80rpx;line-height:80rpx;font-size:32rpx;margin-bottom:20rpx;background:#fff;}
.main_block_title text{border-bottom:4rpx solid #fff;height:76rpx;display:inline-block;padding:0 10rpx;font-weight:bold;}
.main_block_title text.main_block_title_item{border-bottom:4rpx solid #e40064;color:#e40064;}

.goods_list{display:block;margin-top:20rpx;}
.goods_list_row{display:block;background:#fff;padding-bottom:5px;margin-bottom:10px;-webkit-box-shadow:0 5rpx 5rpx #ccc;-moz-box-shadow:0 5rpx 5rpx #ccc;box-shadow:0 5rpx 5rpx #ccc;}
.goods_info{padding:10px;font-size:14px;}
.goods_img{position:relative;}
.goods_img image{display:block;width:100%;height:325rpx;}
.goods_img ul{z-index:10;width:200px;text-align:center;margin:-40px auto 0;}
.goods_img .dtime{text-align:center;position:absolute;width:100%;bottom:0;}
.goods_img .dtime{background-color:rgba(0, 0, 0, 0.6);color:#fff;display:inline-block; box-shadow:none;padding:10rpx 0;font-size:24rpx;}
.goods_img .timeout{position:relative;z-index:10;width:100%;text-align:center;margin:-40px auto 0;background-color:rgba(0, 0, 0, 0.7);color:#fff;height:40px;line-height:40px;}
.goods_img .ltime{width:100%;text-align:center;position:absolute;bottom:10rpx;}
.goods_img .ltime text{background-color:rgba(0, 0, 0, 0.6);color:#fff;display:inline-block; box-shadow:none;padding:10rpx;font-size:24rpx;margin:0 6rpx;}


.goods_btn{border-radius:6rpx;}
.goods_title{font-size:32rpx;height:54rpx;overflow:hidden;}
.goods_tips{display:block;font-size:24rpx;line-height:40rpx;color:#777;}
.goods_vipprice{color:#e40064;font-size:32rpx;}
.goods_price{color:#333;font-size:32rpx;}
.delprice{text-decoration:line-through;font-size:28rpx;}

.fontred{color:#e40064;}


.order_tab .active{color:#e40064;}

.order_tab{text-align:center;height:80rpx;line-height:80rpx;background:#fff;margin-top:10rpx;font-size:28rpx;border-bottom:1rpx solid #eee;}
.order_tab image{width:24rpx;height:24rpx; vertical-align:middle;display:inline-block;}


.goods_cat_list{width:100%; background:#fff;}
.goods_cat_list_li{width:45%;padding:2%;border-right:1rpx solid #eee;border-bottom:1rpx solid #eee;height:470rpx;font-size:28rpx;display:inline-block;}
.goods_cat_list_li image{width:100%;height:160px;}
.no_border{border-right:none;}

.goods_cat_title{font-size:30rpx;padding:10rpx 0;height:75rpx;line-height:40rpx;overflow:hidden;}
.goods_cat_price{width:100%;}
.vipprice{color:#e40064;font-weight:bold;margin-right:10rpx;}
.price{color:#8a8a8a;text-decoration:line-through;font-size:24rpx;}

.cart-icon{position:fixed;width:80rpx;height:80rpx;bottom:150rpx;right:20rpx;}
          .cart-icon image{width:100%;height:100%;}
          .cart-icon span.number{position:absolute;top:-6rpx;right:-6rpx;width:32rpx;height:32rpx;font-size:20rpx;color:white;background-color:rgba(0,0,0,0.8);border-radius:16rpx;text-align:center;line-height:32rpx;}


